def calculate_drug_dosage(weight, strength):
    """Calculate paracetamol dosage volume for children.
    
    Args:
        weight (float): Patient weight in kg (10-100 kg)
        strength (str): Drug strength ('120 mg/5 ml' or '250 mg/5 ml')
    
    Returns:
        float: Volume of drug in ml
    
    Raises:
        ValueError: If weight or strength are invalid
    """
    # Check weight range
    if weight < 10 or weight > 100:
        raise ValueError(f"Weight must be between 10 and 100 kg (got {weight} kg)")
    
    # Check strength format
    valid_strengths = ['120 mg/5 ml', '250 mg/5 ml']
    if strength not in valid_strengths:
        raise ValueError(f"Strength must be one of {valid_strengths} (got '{strength}')")
    
    # Calculate dosage
    dose_per_kg = 15  # mg/kg
    dose = weight * dose_per_kg  # total mg needed
    
    # Extract mg value from strength string
    mg = float(strength.split()[0])
    mg_per_ml = mg / 5
    volume = dose / mg_per_ml
    
    return volume

# Example usage
try:
    weight = 30
    strength = '120 mg/5 ml'
    volume = calculate_drug_dosage(weight, strength)
    print(f"{weight} kg patient requires {volume:.2f} ml of drug with {strength} strength.")
except ValueError as e:
    print(f"Error: {e}")